@model IEnumerable<AT02_4_GaldonMario_Musica.Models.Album>

@{
    ViewData["Title"] = "DiscoArtista";
}

<h1 class="text-light">DiscoArtista</h1>

<p>
    
</p>
<table class="table">
    <thead>
        <tr>
            <th class="text-light">
                Titulo
            </th>
            <th class="text-light">
                Artista
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td class="text-light">
                @Html.DisplayFor(modelItem => item.Title)
            </td>
                <td class="text-light">
                @Html.DisplayFor(modelItem => item.Artist.Name)
            </td>
        </tr>
}
    </tbody>
</table>
